<div id="<%= dom_id invite_link %>">
  <%= invite_link.code %>

  <%= button_to herd_invite_link_path(invite_link.herd, invite_link), method: :delete do %>
    <i class="ph-fill ph-trash"></i>
  <% end %>

  <%= link_to "Edit this invite link", edit_herd_invite_link_path(invite_link.herd, invite_link) %>
</div>
